{
  "name": "ee.apajo.ytrc",
  "displayName": "YouTube Remote Control",
  "version": "1.0.0",
  "description": "Control Your playlists remotely with a mobile device",
  "main": "index.js",
  "author": "Andres Pajo",
  "license": "Apache-2.0",
  "app": {
    "theme": "dark",
    "metrika": false
  },
  "scripts": {
    "gulp": "gulp",
    "build:theme": "node vendor/lib/node_modules/gulp-cli/bin/gulp.js --src=src/theme.json",
    "build:src": "node vendor/lib/node_modules/gulp-cli/bin/gulp.js  --src=src/app.json --concat",
    "build:src:watch": "node vendor/lib/node_modules/gulp-cli/bin/gulp.js  --src=src/app.json --concat --watch"
  },
  "dependencies": {
    "bootstrap": "3.3.7",
    "bootstrap-slider": "10.0.0",
    "command-line-args": "^5.0.1",
    "cordova-android": "^7.0.0",
    "cordova-icon-generator": "0.0.4",
    "cordova-plugin-autostart": "git+https://github.com/ToniKorin/cordova-plugin-autostart.git",
    "cordova-plugin-background-mode": "^0.7.2",
    "cordova-plugin-device": "^2.0.1",
    "cordova-plugin-device-motion-hutchinson": "^1.2.3",
    "cordova-plugin-facebook": "^0.2.2",
    "cordova-plugin-intent": "git+https://github.com/napolitano/cordova-plugin-intent.git",
    "cordova-plugin-vibration": "3.0.1",
    "cordova-plugin-whitelist": "^1.3.3",
    "cordova-plugin-x-socialsharing": "5.2.1",
    "font-awesome": "4.7.0",
    "handlebars": "4.0.11",
    "jquery": "3.3.1"
  },
  "devDependencies": {
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^2.3.1",
    "gulp-cli": "^2.0.1",
    "gulp-concat": "^2.6.1",
    "gulp-cssmin": "^0.1.7",
    "gulp-debug-streams": "^3.0.0",
    "gulp-html-minifier": "^0.1.8",
    "gulp-if": "^2.0.2",
    "gulp-imagemin": "^2.4.0",
    "gulp-jade": "^1.1.0",
    "gulp-less": "^3.3.0",
    "gulp-minify": "^2.1.0",
    "gulp-pug": "^3.2.0",
    "gulp-rename": "^1.2.2",
    "gulp-watch": "^4.3.11",
    "imagemin-pngquant": "^4.2.2",
    "rimraf": "^2.5.4"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-background-mode": {},
      "cordova-plugin-autostart": {},
      "cordova-plugin-facebook": {
        "FACEBOOK_DISPLAY_NAME": "Test",
        "FACEBOOK_APP_ID": "2251019541791490"
      },
      "com.napolitano.cordova.plugin.intent": {}
    },
    "platforms": [
      "android"
    ]
  }
}